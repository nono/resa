%html
  %head
    %script{:src => 'javascripts/jquery.min.js', :type => 'text/javascript'}
    %script{:src => 'javascripts/jquery-ui-1.8.16.custom.min.js', :type => 'text/javascript'}
    %script{:src => 'javascripts/json2.js', :type => 'text/javascript'}
    %script{:src => 'javascripts/underscore-min.js', :type => 'text/javascript'}
    %script{:src => 'javascripts/backbone-min.js', :type => 'text/javascript'}
    %script{:src => 'javascripts/fullcalendar.min.js', :type => 'text/javascript'}
    %script{:src => 'javascripts/app.js', :type => 'text/javascript'}
    %link{:href => 'stylesheets/fullcalendar.css', :rel => 'stylesheet', :type => 'test/css'}
    %link{:href => 'stylesheets/jquery-ui-1.8.16.custom.css', :rel => 'stylesheet', :type => 'test/css'}
  %title= Resa

  %body
    %header
      Resa
    %section#main
    %div#eventDialog.dialog.ui-helper-hidden
      %form
        %div
          %p
            %label Title
            %input#title.field{:type => 'text'}
          %p#rooms
          %p
            %label Begin
            %input#begin.field{:type => 'text'}
          %p
            %label End
            %input#end.field{:type => 'text'}
    %footer
      cody pony rox

